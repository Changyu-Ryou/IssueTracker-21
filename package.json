		"passport": "^0.4.1",
		"passport-github": "^1.1.0",
		"passport-jwt": "^4.0.0",
		"passport-local": "^1.0.0",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-router-dom": "^5.2.0",
		"sequelize": "^6.3.5"
